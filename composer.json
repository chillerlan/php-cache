{
	"name": "chillerlan/php-cache",
	"description": "A psr/simple-cache implementation. PHP 7+",
	"homepage": "https://github.com/codemasher/php-cache",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net"
		}
	],
	"require": {
		"php": ">=7.0.3",
		"psr/simple-cache": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "6.5.*"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\SimpleCache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\SimpleCacheTest\\": "tests"
		}
	},
	"provide": {
		"psr/simple-cache-implementation": "1.0"
	}
}
