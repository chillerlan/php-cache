{
	"name": "chillerlan/php-cache",
	"description": "A psr/simple-cache implementation. PHP 7.4+",
	"homepage": "https://github.com/chillerlan/php-cache",
	"license": "MIT",
	"type": "library",
	"minimum-stability": "stable",
	"keywords": [
		"cache", "psr-16", "php7"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-cache/issues",
		"source": "https://github.com/chillerlan/php-cache"
	},
	"require": {
		"php": "^8.1",
		"psr/log": "^1.1 || ^2.0 || ^3.0",
		"psr/simple-cache": "^1.0",
		"chillerlan/php-settings-container": "^3.0"
	},
	"require-dev": {
		"phan/phan": "^5.4",
		"phpunit/phpunit": "^10.0"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\SimpleCache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\SimpleCacheTest\\": "tests"
		}
	},
	"provide": {
		"psr/simple-cache-implementation": "1.0"
	},
	"scripts": {
		"phpunit": "@php vendor/bin/phpunit",
		"phan": "@php vendor/bin/phan"
	},
	"config": {
		"lock": false,
		"sort-packages": true,
		"platform-check": true
	}
}
