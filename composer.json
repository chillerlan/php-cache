{
	"name": "chillerlan/php-cache",
	"description": "A psr/simple-cache implementation. PHP 7.2+",
	"homepage": "https://github.com/chillerlan/php-cache",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"cache", "psr-16", "php7"
	],
	"authors": [
		{
			"name": "Smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-cache/issues",
		"source": "https://github.com/chillerlan/php-cache"
	},
	"minimum-stability": "stable",
	"require": {
		"php": "^7.2",
		"psr/log": "^1.0",
		"psr/simple-cache": "^1.0",
		"chillerlan/php-traits": "^2.0"
	},
	"require-dev": {
		"chillerlan/php-log": "^2.0",
		"phpunit/phpunit": "^7.3"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\SimpleCache\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\SimpleCacheTest\\": "tests"
		}
	},
	"provide": {
		"psr/simple-cache-implementation": "1.0"
	}
}
